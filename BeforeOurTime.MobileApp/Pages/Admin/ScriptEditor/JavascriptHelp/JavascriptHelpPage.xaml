<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeforeOurTime.MobileApp.Pages.Admin.ScriptEditor.JavascriptHelp.JavascriptHelpPage"
             Title="Javascript Help">
    <ContentPage.Content>
        <StackLayout VerticalOptions="FillAndExpand">
            <StackLayout VerticalOptions="StartAndExpand">
                <Entry Text="var onItemLoad = function(item) { }"
                        Style="{StaticResource botEditorPrimary}"></Entry>
                <Entry Text="var onUse = function(item, itemCommand, origin) { }"
                        Style="{StaticResource botEditorPrimary}"></Entry>
                <Entry Text="botEmote(WorldEmoteType type, string description)"
                        Style="{StaticResource botEditorPrimary}"></Entry>
            </StackLayout>
            <FlexLayout VerticalOptions="End"
                        HorizontalOptions="FillAndExpand">
                <Button Text="Cancel"
                        Style="{StaticResource botButtonPrimary}"
                        Clicked="CancelButton_Clicked"></Button>
            </FlexLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>